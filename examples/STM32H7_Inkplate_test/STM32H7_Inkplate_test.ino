#include "Inkplate6NextGen.h"
#include "clockFunc.c"
#include "img1.h"
#include "img2.h"
Inkplate display(INKPLATE_3BIT);
void setup()
{
  HAL_Init();
  SystemInit();
  //SystemClock_Config360();
  SystemClock_ConfigHSI();
  SystemCoreClockUpdate();
  display.begin();
  //display.setCursor(100, 0);
  //display.setTextColor(0, 7);
  //display.setTextSize(4);
  //display.print("This is test");
  //grad();
  display.drawBitmap3Bit(0, 0, pic1, 800, 592);
  display.display();
  delay(5000);
  display.drawBitmap3Bit(0, 0, pic2, 800, 592);
  display.display();
}

void loop() {

}

void grad()
{
  for(int i = 0; i < 8; i++)
  {
    display.fillRect(100*i, 200, 100, 300, i);
  }
}




